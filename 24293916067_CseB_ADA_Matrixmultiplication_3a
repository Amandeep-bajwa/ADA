#include <stdio.h>
#include <stdlib.h>
#include <time.h>

void multiplyMatrices(int n, int A[n][n], int B[n][n], int C[n][n]) {
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            C[i][j] = 0;
            for (int k = 0; k < n; k++) {
                C[i][j] += A[i][k] * B[k][j];
            }
        }
    }
}

void generateMatrix(int n, int M[n][n]) {
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            M[i][j] = rand() % 10;
        }
    }
}

int main() {
    int sizes[] = {100, 200, 300, 400, 500};
    int numSizes = sizeof(sizes) / sizeof(sizes[0]);
    srand(time(NULL));

    for (int s = 0; s < numSizes; s++) {
        int n = sizes[s];
        int A[500][500], B[500][500], C[500][500];

        generateMatrix(n, A);
        generateMatrix(n, B);

        clock_t start = clock();
        multiplyMatrices(n, A, B, C);
        clock_t end = clock();

        double time_taken = ((double)(end - start)) / CLOCKS_PER_SEC;
        printf("Matrix size: %d x %d -> Time taken: %f seconds\n", n, n, time_taken);
    }

    return 0;
}

#include <stdio.h>
#include <time.h>

int main() {
    int n_values[] = {10, 20, 30, 40, 50, 60};
    int size = sizeof(n_values) / sizeof(n_values[0]);
    clock_t start, end;
    double time_taken;

    for (int k = 0; k < size; k++) {
        int n = n_values[k];
        long long fib[n+1];
        fib[0] = 0;
        if (n > 0) fib[1] = 1;

        for (int i = 2; i <= n; i++) {
            fib[i] = fib[i-1] + fib[i-2];
        }

        printf("\n=== Fibonacci Series for n = %d ===\n", n);
        for (int i = 0; i < n; i++) {
            printf("%lld ", fib[i]);
        }
        printf("\n");

        start = clock();
        long long nth = fib[n];
        end = clock();
        time_taken = ((double)(end - start)) / CLOCKS_PER_SEC;

        printf("The %dth Fibonacci number = %lld\n", n, nth);
        printf("Execution time: %f seconds\n", time_taken);
    }

    return 0;
}
